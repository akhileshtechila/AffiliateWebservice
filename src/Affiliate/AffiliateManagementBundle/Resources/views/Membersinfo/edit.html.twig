{% extends '::baseadmindemo.html.twig' %}
{% block title %}Members Information{% endblock %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}
{% block javascripts %}
   {{ parent() }}    
    
{% endblock %}

{% block body -%}

    <!-- Main content -->

                        <div class="col-md-6">
                            <!-- general form elements disabled -->
                            <div class="box box-warning">
                                <div class="box-header">
                                    <h3 class="box-title">Members Info </h3>
                                </div><!-- /.box-header -->
                                <div class="box-body">
                                    {{form_start(edit_form, {'enctype': 'multipart/form-data' ,'attr':{'class':'' ,'role':'form'}}) }}
                                        <!-- text input -->
                                        <div class="form-group">
                                            <label>First Name</label>
                                             {{ form_widget(edit_form.fName,{'attr':{'class':"form-control"}}) }} 
                                        </div>
                                        
                                         <!-- text input -->
                                        <div class="form-group">
                                            <label>Last Name</label>
                                           {{ form_widget(edit_form.lName,{'attr':{'class':"form-control"}}) }}
                                            
                                        </div>
                                         
                                   
                                           <div class="form-group">
                                            <label>Email</label>
                                            {{ form_widget(edit_form.email,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>
                                           
                                        <div class="form-group">
                                            <label>State</label>
                                            {{ form_widget(edit_form.state,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>
                                           
                                        <div class="form-group">
                                            <label>Country</label>
                                            {{ form_widget(edit_form.country,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>
                                           
                                         <div class="form-group">
                                            <label>Zipcode</label>
                                            {{ form_widget(edit_form.zipCode,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>
                                           
                                        <div class="form-group">
                                            <label>Mobile</label>
                                            {{ form_widget(edit_form.mobile,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>
                                           
                                        <div class="form-group">
                                            <label>User Type</label>
                                            {{ form_widget(edit_form.Userinfo,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>
                                           
                                        <div class="form-group">
                                            <label>Device Token</label>
                                            {{ form_widget(edit_form.deviceToken,{'attr':{'class':"form-control"}}) }} 
                                           <!-- <input type="text" class="form-control" placeholder="Enter ..."/>-->
                                        </div>   
                                           
                                          {{ form_widget(edit_form._token) }}
                                           <div class="form-group">
                                             <button type="submit"  class="btn btn-success"><i class="icon-ok icon-green"></i> Create</button>
                                        </div>
                                          
                                    {{form_end(edit_form, {'render_rest': false}) }} 
                                </div><!-- /.box-body -->
                            </div><!-- /.box -->
                            
                            <ul class="record_actions">
                                <li>
                                    <a href="{{ path('membersinfo') }}">
                                        Back to the list
                                    </a>
                                </li>
                            </ul>
                            
                        </div><!--/.col (right) -->

          

{% endblock %}
