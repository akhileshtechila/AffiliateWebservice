{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Affiliateinfo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
              <tr>
                <th>Added By</th>
                <td>{{ entity.Userinfo.email }}</td>
            </tr>
            <tr>
                <th>Fname</th>
                <td>{{ entity.fName }}</td>
            </tr>
            <tr>
                <th>Lname</th>
                <td>{{ entity.lName }}</td>
            </tr>

            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ entity.state }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ entity.country }}</td>
            </tr>
            <tr>
                <th>Zipcode</th>
                <td>{{ entity.zipCode }}</td>
            </tr>
            <tr>
                <th>Mobile</th>
                <td>{{ entity.mobile }}</td>
            </tr>


        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('membersinfo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('membersinfo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <a href="{{ path('membersinfo_delete', { 'id': entity.id }) }}">
            Delete
        </a>
    </li>
</ul>
{% endblock %}
