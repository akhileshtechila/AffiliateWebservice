{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Deal</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Dname</th>
                <td>{{ entity.dName }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Dealduration</th>
                <td>{{ entity.dealDuration }}</td>
            </tr>

        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('deal') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('deal_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
